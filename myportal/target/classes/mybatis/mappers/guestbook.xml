<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="guestbook">
	<select id="selectAll"
		resultType="com.bitacademy.myportal.vo.GuestbookVo">
	<![CDATA[
	SELECT no, 
		name, 
		password, 
		content,
		reg_date as regDate
	FROM 
		guestbook
	]]>
	</select>
</mapper>